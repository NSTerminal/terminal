{
    "editor.formatOnType": true,
    "[python]": {
        "editor.rulers": [
            72,
            79
        ]
    },
    "[c]": {
        "editor.rulers": [
            120
        ]
    },
    "[cpp]": {
        "editor.rulers": [
            120
        ]
    },
    "C_Cpp.default.cppStandard": "c++20",
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/core/lib"
    ],
    "todo-tree.filtering.excludeGlobs": [
        "**/lib/*/**"
    ],
    "cmake.configureOnOpen": true,
    "cmake.configureOnEdit": true,
    "cmake.sourceDirectory": "${workspaceFolder}/core",
    "cmake.buildDirectory": "${workspaceFolder}/core/build",
    "python.analysis.extraPaths": [
        "${workspaceFolder}/core/lib"
    ],
    "autoDocstring.docstringFormat": "numpy",
    "doxdocgen.c.commentPrefix": "/// ",
    "doxdocgen.c.firstLine": "",
    "doxdocgen.c.lastLine": "",
    "doxdocgen.c.triggerSequence": "///",
    "doxdocgen.cpp.ctorText": "{name} constructor",
    "doxdocgen.cpp.dtorText": "{name} destructor",
    "doxdocgen.cpp.tparamTemplate": "<typeparam name=\"{param}\"></typeparam>",
    "doxdocgen.generic.briefTemplate": "<summary>\n{text}\n</summary>",
    "doxdocgen.generic.includeTypeAtReturn": false,
    "doxdocgen.generic.paramTemplate": "<param name=\"{param}\"></param>",
    "doxdocgen.generic.returnTemplate": "<returns></returns>",
    "doxdocgen.generic.commandSuggestion": false,
    "files.associations": {
        "*.tcc": "cpp",
        "algorithm": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "cctype": "cpp",
        "charconv": "cpp",
